@model AppIntegrador.Models.TodasLasPreguntas


@foreach (var pregunta in Model.Preguntas)
{
    if (pregunta.Pregunta.Tipo == "U")
    {
        {Html.RenderPartial("PreguntaConOpciones", pregunta);}
    }
    else if(pregunta.Pregunta.Tipo == "S")
    {
        {Html.RenderPartial("PreguntaSiNoNr", pregunta);}
    }
}

